@prefix log: <http://www.w3.org/2000/10/swap/log#>.
@prefix string: <http://www.w3.org/2000/10/swap/string#>.
@prefix : <http://example.org/ns#>.

{
    ?s log:graphAsk ?o .
} <= {
    (?s) string:concatenation ?c .
    ?c log:ask ?res .
    ?res log:parsedAsN3 ?o .
} .

{
    :p :o ?o .
} <= {
    { :a :b :c } log:graphAsk [ log:includes { :a :b ?o } ] .
} .

{
    :p :o ?o .
} log:query {
    :result :is ?o .
}.
